{
  "imports": {
    "std/": "https://deno.land/std@0.207.0/"
  },
  "lock": false,
  "tasks": {
    "build": "deno task build:hasundue && sudo nixos-rebuild --flake '.?submodules=2'",
    "build:hasundue": "deno run -A users/hasundue/**/*.build.ts",
    "check": "deno task check:deno",
    "check:deno": "deno fmt --check && deno lint && deno check -q ./**/lib/*.ts",
    "switch": "deno task build switch",
    "test": "deno task -q check && deno task build test",
    "update": "nix flake update ."
  }
}
