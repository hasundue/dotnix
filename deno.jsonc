{
  "tasks": {
    "build": "deno task build:hasundue && sudo nixos-rebuild",
    "build:hasundue": "deno run -A users/hasundue/**/*.build.ts",
    "check": "deno check ./**/*.ts",
    "switch": "deno task build switch --flake .",
    "test": "deno task test:deno && deno task build test --flake .",
    "test:deno": "deno test -A",
    "update": "nix flake update ."
  },
  "lock": false
}
